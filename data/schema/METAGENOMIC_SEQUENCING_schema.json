{
    "$id": "https://www.ebi.ac.uk/exeriment.checklist.ena.schema.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "allOf": [
        {
            "checklist_id": {
                "_example": "EXC000002",
                "_required": true,
                "default": "EXC000002",
                "description": "checklist identifier",
                "minLength": 3,
                "type": "string"
            }
        },
        {
            "checklist_name": {
                "_example": "METAGENOMIC sequencing specific",
                "_required": false,
                "default": "METAGENOMIC sequencing specific",
                "description": "A short descriptive name to human identify",
                "minLength": 3,
                "type": "string"
            }
        },
        {
            "checklist_version": {
                "_comment": "Version of the format v[[::digit::]] e..g. v2",
                "_example": "v1",
                "_required": true,
                "default": "v1",
                "description": "The version of this checklist, allows us to validate the write records against the right checklist version",
                "type": "integer"
            }
        },
        {
            "checklist_group": {
                "_example": "METAGENOMIC",
                "_required": false,
                "default": "METAGENOMIC",
                "description": "The collection of the experiment checklists",
                "type": "string"
            }
        },
        {
            "experiment_type": {
                "_example": "METAGENOMIC_SEQUENCING",
                "_required": true,
                "default": "METAGENOMIC_SEQUENCING",
                "description": "The broad type of sequencing experiment performed. A mixture of library strategy and source",
                "type": "string"
            }
        },
        {
            "target_loci": {
                "_example": "",
                "_required": false,
                "default": "",
                "description": "Names the gene(s) or locus(loci) or other genomic feature(s) targeted by the sequence.[INSDC adapted]",
                "enum": [
                    "16S rRNA",
                    "18S rRNA",
                    "28S rRNA",
                    "COX1",
                    "ITS1-5.8S-ITS2",
                    "RBCL",
                    "exome",
                    "matK",
                    "other"
                ],
                "type": "string"
            }
        },
        {
            "target_subfragment": {
                "_example": "",
                "_required": false,
                "default": "",
                "description": "",
                "ontology": "https://www.ebi.ac.uk/ols/ontologies/XXX",
                "type": "string"
            }
        },
        {
            "design_description": {
                "_example": "",
                "_required": true,
                "default": "",
                "description": "The library design including library properties, layout, protocol, targeting information, and spot and gap descriptors.",
                "type": "string"
            }
        },
        {
            "if": {
                "properties": {
                    "instrument": {
                        "const": "unspecified"
                    }
                }
            },
            "then": {
                "required": [
                    "instrument_platform"
                ]
            }
        },
        {
            "if": {
                "properties": {
                    "library_selection": {
                        "pattern": "PCR"
                    }
                }
            },
            "then": {
                "required": [
                    "pcr_primers"
                ]
            }
        },
        {
            "if": {
                "properties": {
                    "instrument_platform": {
                        "const": "LS454"
                    }
                }
            },
            "then": {
                "properties": {
                    "instrument": {
                        "enum": [
                            "454 GS FLX Titanium",
                            "454 GS",
                            "454 GS FLX",
                            "unspecified",
                            "454 GS 20",
                            "454 GS FLX+",
                            "454 GS Junior"
                        ]
                    }
                }
            }
        },
        {
            "if": {
                "properties": {
                    "instrument_platform": {
                        "const": "ILLUMINA"
                    }
                }
            },
            "then": {
                "properties": {
                    "instrument": {
                        "enum": [
                            "Illumina Genome Analyzer II",
                            "Illumina HiSeq 1000",
                            "Illumina MiSeq",
                            "HiSeq X Ten",
                            "Illumina iSeq 100",
                            "Illumina NovaSeq X",
                            "NextSeq 1000",
                            "Illumina HiSeq 2000",
                            "Illumina HiSeq 1500",
                            "Illumina HiSeq 2500",
                            "Illumina Genome Analyzer IIx",
                            "NextSeq 500",
                            "Illumina HiSeq 3000",
                            "Illumina Genome Analyzer",
                            "Illumina MiniSeq",
                            "Illumina HiScanSQ",
                            "Illumina HiSeq 4000",
                            "Illumina HiSeq X",
                            "unspecified",
                            "NextSeq 2000",
                            "HiSeq X Five",
                            "Illumina NovaSeq 6000",
                            "NextSeq 550"
                        ]
                    }
                }
            }
        },
        {
